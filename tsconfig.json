{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "typeRoots": ["./src/types"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "front-end/src", "module_declarations.d.ts", "webpack.config.js", "Tests", "react-app-env.d.ts", "front-end/src/reportWebVitals.ts", "import-jpeg.d.ts", "front-end/src/index.tsx", "front-end/src/App.tsx.tsx"  ]
}
